<!DOCTYPE html>
<html lang="en">
<head>
  <%- include('partials/head'); %>
</head>
<body class="container">

<main>
  <div class="row">
    <div class="col-sm-8">
      <div class="jumbotron">
        <h1>skateboarding-shop</h1>
        <h6>protecting skate culture</h6>
        <table class="sortable">
          <thead>
            <tr>
              <th>Nr</th>
              <th><a href="/nameSort">Name</a></th>
              <th>Image</th>
              <th>Price</th>
              <th>Link</th>
            </tr>
          </thead>
          <% itemList.forEach(function(item) { %>
            <tr>
              <td><%= item[0] %></td>
              <td><%= item[1] %></td>
              <td><img src="<%= item[2] %>" alt="itmimg"/></td>
              <td><%= item[3] %></td>
              <td><a href="<%= item[4] %>">View</a></td>
            </tr>
          <% }); %>
        </table>
      </div>
    </div>

    <div class="col-sm-4">
      <div class="well">
        <h3>Filter</h3>
      </div>
    </div>
  </div>
</main>

</body>
</html>